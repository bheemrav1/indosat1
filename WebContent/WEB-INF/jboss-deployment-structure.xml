           <include path="java**" />
         </imports>
         <imports>
           <include path="javax**" />
         </imports>
     </module>
        <module name="saturn.lib">
        <imports>
        <include path="META-INF**"/>
        </imports>
         <imports>
           <include path="org**" />
         </imports>
         <imports>
           <include path="com**" />
         </imports>
         <imports>
           <include path="java**" />
         </imports>
         <imports>
           <include path="javax**" />
         </imports>
		</module>
     </dependencies>
